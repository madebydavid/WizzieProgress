        "dominikzogg/doctrine-orm-manager-registry-provider": "~1.1",
        "symfony/yaml": "2.4.*@dev",
        "symfony/form": "~2.3",
        "symfony/validator":  "~2.3",
        "symfony/config": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/locale": "~2.3",
