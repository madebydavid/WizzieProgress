{
    "name": "madebydavid/wizzie-progress",
    "description": "W2C3 App to track students' progress",
    "authors": [
        {
            "name": "madebydavid",
            "email": "sutherland.dave@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
